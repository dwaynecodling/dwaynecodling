{
  "name": "template",
  "version": "1.0.0",
  "description": "Dashboard app",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass scss/main.scss dwaynesite/public_html/assets/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss dwaynesite/public_html/assets/css/style.comp.css",
    "concat:css": "concat -o dwaynesite/public_html/assets/css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' dwaynesite/public_html/assets/css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass dwaynesite/public_html/assets/css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "preinstall": "npx npm-force-resolutions"
  },
  "author": "Dwayne",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "concat": "^1.0.3",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  },
  "dependencies": {
    "live-server": "^1.2.1"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  }
}
